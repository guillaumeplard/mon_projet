<?xml version="1.0" encoding="utf-8"?>
<!-- src/AppBundle/Resources/config/doctrine/User.orm.xml -->
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="gaelicBundle\Entity\Contact" table="Contact">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="Sujet" type="text" column="Sujet"/>
        <field name="Message" type="text" column="Message"/>
        <field name="Email" type="text" column="Email"/>
        <field name="Nom" type="text" column="Nom"/>
        <field name="Prenom" type="text" column="Prenom"/>
    </entity>
</doctrine-mapping>